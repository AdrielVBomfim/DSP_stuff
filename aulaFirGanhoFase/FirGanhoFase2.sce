fa = 8000;
f = 500;
k = 1;

//z0 = exp(%i*2*%pi*f/fa);
//h0 = (1-0.1*z0^(-1))/(1-0.9*z0^(-1)+0.5*z0^(-2));

//real(h0)
//imag(h0)

for f = 0:4000
  z0 = exp(%i*2*%pi*(f/fa));
  h0 = (1-0.1*z0^(-1))/(1-0.9*z0^(-1)+0.5*z0^(-2));
  M(k) = abs(h0);
  F(k) = atan(imag(h0)/real(h0));
  k = k + 1;
end

plot((0:4000), M)
plot((0:4000), F)